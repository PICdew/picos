/**
 * This is a program that set the date
 */
sprint("Month: ");
cr();
m = getd();
putd(m);
e = getch();
if(e > 0x2f)
{
	if(e < 0x3a)
	{
		e = e - 0x30;
		m = m*10 + e;
		clear();
		sprint("Month: ");
		cr();
		putd(m);
		e = getch();
	}
}
clear();
sprint("Day: ");
d = getd();
putd(d);
e = getch();
if(e > 0x2f)
{
	if(e < 0x3a)
	{
		e = e - 0x30;
		d = d*10 + e;
		clear();
		sprint("Day: ");
		putd(d);
		e = getch();
	}
}
setdate(m,d);
clear();
sprint("Date Set!");

