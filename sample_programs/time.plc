/**
 * Forces 2 digits to be used.
 */
def <two_digit>
{
  two_digit = pop();
  if(two_digit < 10)
    {
      putd(0);
    }
  putd(two_digit);
}

/**
 * This is a program that will display the date
 */
def <main>
{
  putd(time('m'));
  putch('/');
  putd(time('d'));
  cr();
  putd(time('H'));
  putch(':');
  time('M');
  call <two_digit>;
}
