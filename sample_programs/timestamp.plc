def <three_digit>
{
  t = pop();
  if(t < 100)
    {
      fputd(0);
      if(t < 10)
	{
	  fputd(0);
	}
    }
  fputd(t);
}

def <two_digit>
{
  t = pop();
  if(t < 10)
    {
      fputd(0);
    }
  fputd(t);
}

def <main>
{
  fputd(2);
  time('Y');
  call <three_digit>;
  time('m');
  call <two_digit>;
  time('d');
  call <two_digit>;
  fputch('T');
  time('H');
  call <two_digit>;
  time('M');
  call <two_digit>;
  fflush();

}
